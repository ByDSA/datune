{
  "name": "@datune/midi",
  "version": "0.0.0",
  "description": "Datune - the midi",
  "author": "Daniel Sales √Ålvarez <danisales.es@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ByDSA/datune.git",
    "directory": "packages/midi"
  },
  "sideEffects": false,
  "scripts": {
    "test": "bash ../../scripts/test.sh",
    "build": "bash ../../scripts/build.sh",
    "clean": "bash ../../scripts/clean.sh",
    "publish": "bash ../../scripts/publish.sh",
    "gen": "cd tests/gen && echo Transpiling... && tsc && echo Executing... && node gen.js && echo Done! && npm run genclean",
    "genclean": "cd tests/gen && find . -type f -name '*.d.ts' -delete && find . -type f -name '*.js' -delete && find . -type f -name '*.js.map' -delete"
  },
  "dependencies": {
    "@datune/utils": "file:../utils/dist",
    "@datune/core": "file:../core/dist",
    "@datune/analyzer": "file:../analyzer/dist",
    "@tonejs/midi": "^2.0.25",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "tslib": "^1.10.0",
    "jest-expect-message": "^1.0.2",
    "@types/jest-expect-message": "^1.0.3",
    "@types/jest": "^25.2.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^3.8.3"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  }
}
