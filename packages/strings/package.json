{
  "private": true,
  "name": "@datune/strings",
  "version": "0.0.1",
  "description": "Datune - strings",
  "license": "UNLICENSED",
  "author": "Daniel Sales √Ålvarez <danisales.es@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ByDSA/datune.git",
    "directory": "packages/strings"
  },
  "scripts": {
    "build": "pnpm build:clean && pnpm build:raw",
    "build:clean": "rm -rf dist",
    "build:raw": "tsc -p tsconfig-build.json && tsc-alias -p tsconfig-build.json",
    "prepublishOnly": "pnpm test:coverage && pnpm lint:fix && pnpm build",
    "postpublish": "pnpm build:clean",
    "release": "standard-version",
    "release:push": "git push && git push --tags && pnpm docs:push",
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@datune/core": "file:../core/dist",
    "@datune/midi": "file:../midi/dist",
    "@datune/utils": "file:../utils/dist",
    "chevrotain": "workspace:./lib/chevrotain",
    "immutable": "5.0.3"
  },
  "devDependencies": {
    "@chevrotain/types": "11.0.3",
    "@datune/coreDev": "file:../core/"
  },
  "standard-version": {
    "scripts": {
      "postcommit": "pnpm docs && pnpm release:push"
    }
  }
}
